@article{Liu2011d,
abstract = {Chronic mucocutaneous candidiasis disease (CMCD) may be caused by autosomal dominant (AD) IL-17F deficiency or autosomal recessive (AR) IL-17RA deficiency. Here, using whole-exome sequencing, we identified heterozygous germline mutations in STAT1 in 47 patients from 20 kindreds with AD CMCD. Previously described heterozygous STAT1 mutant alleles are loss-of-function and cause AD predisposition to mycobacterial disease caused by impaired STAT1-dependent cellular responses to IFN-$\gamma$. Other loss-of-function STAT1 alleles cause AR predisposition to intracellular bacterial and viral diseases, caused by impaired STAT1-dependent responses to IFN-$\alpha$/$\beta$, IFN-$\gamma$, IFN-$\lambda$, and IL-27. In contrast, the 12 AD CMCD-inducing STAT1 mutant alleles described here are gain-of-function and increase STAT1-dependent cellular responses to these cytokines, and to cytokines that predominantly activate STAT3, such as IL-6 and IL-21. All of these mutations affect the coiled-coil domain and impair the nuclear dephosphorylation of activated STAT1, accounting for their gain-of-function and dominance. Stronger cellular responses to the STAT1-dependent IL-17 inhibitors IFN-$\alpha$/$\beta$, IFN-$\gamma$, and IL-27, and stronger STAT1 activation in response to the STAT3-dependent IL-17 inducers IL-6 and IL-21, hinder the development of T cells producing IL-17A, IL-17F, and IL-22. Gain-of-function STAT1 alleles therefore cause AD CMCD by impairing IL-17 immunity.},
author = {Liu, Luyan and Okada, Satoshi and Kong, Xiao-Fei and Kreins, Alexandra Y and Cypowyj, Sophie and Abhyankar, Avinash and Toubiana, Julie and Itan, Yuval and Audry, Magali and Nitschke, Patrick and Masson, C{\'{e}}cile and Toth, Beata and Flatot, J{\'{e}}rome and Migaud, M{\'{e}}lanie and Chrabieh, Maya and Kochetkov, Tatiana and Bolze, Alexandre and Borghesi, Alessandro and Toulon, Antoine and Hiller, Julia and Eyerich, Stefanie and Eyerich, Kilian and Gul{\'{a}}csy, Vera and Chernyshova, Ludmyla and Chernyshov, Viktor and Bondarenko, Anastasia and Grimaldo, Rosa Mar{\'{i}}a Cort{\'{e}}s and Blancas-Galicia, Lizbeth and Beas, Ileana Maria Madrigal and Roesler, Joachim and Magdorf, Klaus and Engelhard, Dan and Thumerelle, Caroline and Burgel, Pierre-R{\'{e}}gis and Hoernes, Miriam and Drexel, Barbara and Seger, Reinhard and Kusuma, Theresia and Jansson, Annette F and Sawalle-Belohradsky, Julie and Belohradsky, Bernd and Jouanguy, Emmanuelle and Bustamante, Jacinta and Bu{\'{e}}, M{\'{e}}lanie and Karin, Nathan and Wildbaum, Gizi and Bodemer, Christine and Lortholary, Olivier and Fischer, Alain and Blanche, St{\'{e}}phane and Al-Muhsen, Saleh and Reichenbach, Janine and Kobayashi, Masao and Rosales, Francisco Espinosa and Lozano, Carlos Torres and Kilic, Sara Sebnem and Oleastro, Matias and Etzioni, Amos and Traidl-Hoffmann, Claudia and Renner, Ellen D and Abel, Laurent and Picard, Capucine and Mar{\'{o}}di, L{\'{a}}szl{\'{o}} and Boisson-Dupuis, St{\'{e}}phanie and Puel, Anne and Casanova, Jean-Laurent},
doi = {10.1084/jem.20110958},
file = {:home/raony/Articles/exome studies/J Exp Med-2011-Liu-1635-48.pdf:pdf},
isbn = {1540-9538 (Electronic)$\backslash$r0022-1007 (Linking)},
issn = {0022-1007},
