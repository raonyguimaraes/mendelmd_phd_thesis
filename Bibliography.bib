journal = {Nature},
keywords = {Acetaminophen,Acetaminophen: metabolism,Acetaminophen: pharmacology,Acetaminophen: urine,Animals,Biological,Environment,Individuality,Liver,Liver: drug effects,Liver: metabolism,Liver: pathology,Magnetic Resonance Spectroscopy,Male,Models,Pharmacogenetics,Phenotype,Rats,Sprague-Dawley},
month = {apr},
