journal = {PLoS genetics},
keywords = {Algorithms,Area Under Curve,Artificial Intelligence,Databases,Diabetes Mellitus,Genetic,Genetic Markers,Genetic Predisposition to Disease,Genome-Wide Association Study,Genome-Wide Association Study: methods,Humans,Models,Oligonucleotide Array Sequence Analysis,Polymorphism,ROC Curve,Reproducibility of Results,Risk Assessment,Risk Assessment: methods,Single Nucleotide,Type 1,Type 1: genetics},
month = {oct},
