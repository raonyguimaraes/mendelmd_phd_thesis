issn = {1476-4687},
journal = {Nature},
keywords = {Agriculture,Agriculture: history,Alleles,Biological Evolution,Caves,European Continental Ancestry Group,European Continental Ancestry Group: genetics,Eye Color,Eye Color: genetics,Fossils,Genome, Human,Genome, Human: genetics,Genomics,History, Ancient,Humans,Immunity,Immunity: genetics,Lactose Intolerance,Lactose Intolerance: genetics,Male,Pigmentation,Pigmentation: genetics,Polymorphism, Single Nucleotide,Polymorphism, Single Nucleotide: genetics,Principal Component Analysis,Skeleton,Skin Pigmentation,Skin Pigmentation: genetics,Spain,Spain: ethnology},
number = {7491},
