journal = {Human molecular genetics},
keywords = {Animals,Base Sequence,Blotting, Western,Ciliary Motility Disorders,Ciliary Motility Disorders: etiology,Ciliary Motility Disorders: metabolism,DNA Primers,DNA Primers: chemistry,DNA Probes,DNA Probes: chemistry,Dyneins,Dyneins: genetics,Dyneins: metabolism,Female,Gene Deletion,Gene Targeting,Humans,Immunoenzyme Techniques,In Situ Hybridization,Infertility, Male,Infertility, Male: etiology,Infertility, Male: metabolism,Male,Mice,Mice, Knockout,Molecular Sequence Data,Oligospermia,Oligospermia: etiology,Oligospermia: metabolism,Reverse Transcriptase Polymerase Chain Reaction,Spermatozoa,Spermatozoa: physiology},
