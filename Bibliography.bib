@article{Calhau2011,
abstract = {Geralmente, sistemas s{\~{a}}o constru{\'{i}}dos por diferentes desenvolvedores, em diferentes momentos, sem uma preocupa{\c{c}}{\~{a}}o com o estabelecimento de um significado comum aos itens comunicados pelos mesmos. Em geral, cada sistema {\'{e}} executado separadamente e implementa o seu pr{\'{o}}prio modelo de dados e de processo. Esses modelos n{\~{a}}o s{\~{a}}o compartilhados entre os sistemas, o que abre espa{\c{c}}o para diversos tipos de conflitos, incluindo conflitos sint{\'{a}}ticos e sem{\^{a}}nticos. Essa heterogeneidade {\'{e}} considerada uma das maiores dificuldades no problema da integra{\c{c}}{\~{a}}o. Neste contexto, ontologias podem ser usadas como uma interl{\'{i}}ngua para mapear conceitos e servi{\c{c}}os usados por diferentes sistemas, que acessariam dados e servi{\c{c}}os por meio de ontologias compartilhadas. Neste presente trabalho defende-se a ideia de que a integra{\c{c}}{\~{a}}o sem{\^{a}}ntica {\'{e}} uma tarefa complexa e bastante subjetiva e, por isso, deve ocorrer em um n{\'{i}}vel mais alto de abstra{\c{c}}{\~{a}}o. Tomando por base esta considera{\c{c}}{\~{a}}o, foi desenvolvida OBA-SI (Ontology- Based Approach for Semantic Integration), uma abordagem de integra{\c{c}}{\~{a}}o sem{\^{a}}ntica de sistemas que concentra esfor{\c{c}}os na modelagem conceitual e na an{\'{a}}lise dos requisitos de integra{\c{c}}{\~{a}}o. Nessa abordagem, a integra{\c{c}}{\~{a}}o sem{\^{a}}ntica {\'{e}} realizada em um alto n{\'{i}}vel de abstra{\c{c}}{\~{a}}o, provendo acordo sem{\^{a}}ntico entre os sistemas no n{\'{i}}vel conceitual. OBA-SI lida com a integra{\c{c}}{\~{a}}o nas tr{\^{e}}s camadas de integra{\c{c}}{\~{a}}o: dados, servi{\c{c}}os e processos. Para tal, modelos conceituais dos sistemas (representando sua estrutura e comportamento), bem como do processo de neg{\'{o}}cio por eles apoiado, s{\~{a}}o comparados {\`{a}} luz de ontologias, usadas para atribuir sem{\^{a}}ntica aos itens compartilhados entre os sistemas no apoio ao processo de neg{\'{o}}cio considerado. Os modelos s{\~{a}}o compatibilizados por meio de mapeamentos entre seus elementos. Todo esse processo de atribui{\c{c}}{\~{a}}o de sem{\^{a}}ntica e uso de ontologias {\'{e}} independente da solu{\c{c}}{\~{a}}o da integra{\c{c}}{\~{a}}o. A fim de se testar a presente abordagem, foi realizado um estudo de caso no qual ela foi aplicada na integra{\c{c}}{\~{a}}o sem{\^{a}}ntica de dois sistemas de Ger{\^{e}}ncia de Configura{\c{c}}{\~{a}}o de Software, usando ontologias de dom{\'{i}}nio e de tarefa.},
author = {Calhau, Rodrigo Fernandes},
keywords = {Ger{\^{e}}ncia de Configura{\c{c}}{\~{a}}o de Software,Integra{\c{c}}{\~{a}}o Sem{\^{a}}ntica de Sistemas,Interoperabilidade Sem{\^{a}}ntica,Ontologias},
pages = {160},
title = {{Uma abordagem baseada em ontologias para integra{\c{c}}{\~{a}}o sem{\^{a}}ntica de sistemas}},
year = {2011}
}
