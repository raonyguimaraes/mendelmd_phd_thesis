journal = {Annual review of genomics and human genetics},
keywords = {Animals,DNA Copy Number Variations,Disease,Disease: genetics,Gene Dosage,Gene Expression Regulation,Humans,Regulatory Elements,Transcriptional},
month = {sep},
pages = {53--72},
pmid = {21756107},
title = {{Copy-number variations, noncoding sequences, and human phenotypes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21756107},
volume = {12},
year = {2011}
}
