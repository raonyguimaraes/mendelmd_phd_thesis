journal = {Nature reviews. Genetics},
keywords = {Alleles,Animals,Genetic Diseases,Genetic Predisposition to Disease,Genetic Variation,Genome,Heritable,Human,Humans,Inborn,Inborn: genetics,Quantitative Trait,Quantitative Trait Loci},
