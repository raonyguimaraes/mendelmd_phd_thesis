issn = {1537-6605},
journal = {American journal of human genetics},
keywords = {Exome,Eye Diseases,Female,Genetic,Genetic Diseases,Hereditary,Hereditary: genetics,Humans,Male,Membrane Proteins,Membrane Proteins: analysis,Membrane Proteins: genetics,Middle Aged,Mutation,Myopia,Myopia: genetics,Night Blindness,Night Blindness: genetics,Polymorphism,Retina,Retina: chemistry,X-Linked,X-Linked: genetics},
