journal = {Genetics in medicine : official journal of the American College of Medical Genetics},
keywords = {causal mutation wes,clinical practice,clinical u},
month = {feb},
number = {November 2013},
pages = {1--3},
pmid = {24525916},
title = {{Clinical whole-exome sequencing: are we there yet?}},
