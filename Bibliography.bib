isbn = {1553-7404},
issn = {15537390},
journal = {PLoS Genetics},
number = {3},
pages = {1--7},
pmid = {21455487},
title = {{Whole-exome re-sequencing in a family quartet identifies POP1 mutations as the cause of a novel skeletal dysplasia}},
volume = {7},
year = {2011}
}
