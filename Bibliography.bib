journal = {Nature medicine},
keywords = {Animals,Antigens, CD44,Antigens, CD44: drug effects,Gene Expression Regulation, Neoplastic,Gene Expression Regulation, Neoplastic: drug effec,Male,Mice,MicroRNAs,MicroRNAs: therapeutic use,Neoplasm Metastasis,Neoplasm Transplantation,Neoplastic Stem Cells,Neoplastic Stem Cells: drug effects,Prostatic Neoplasms,Prostatic Neoplasms: drug therapy,Reverse Transcriptase Polymerase Chain Reaction},
