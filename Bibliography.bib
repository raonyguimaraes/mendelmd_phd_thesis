isbn = {1367-4811 (Linking)},
issn = {13674803},
journal = {Bioinformatics},
number = {16},
pages = {1925--1933},
pmid = {23782614},
title = {{Crowdsourcing for bioinformatics}},
volume = {29},
year = {2013}
}
