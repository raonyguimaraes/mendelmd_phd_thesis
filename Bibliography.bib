journal = {American journal of human genetics},
keywords = {80 and over,Adolescent,Adult,Aged,Breast Neoplasms,Breast Neoplasms: epidemiology,Breast Neoplasms: genetics,Child,Female,Genetic Predisposition to Disease,Genetic Variation,Genetics,Genome,Genomics,Genotype,Human,Humans,Infant,Middle Aged,Models,Newborn,Population,Preschool,Reproducibility of Results,Risk,SEER Program,Statistical},
month = {dec},
