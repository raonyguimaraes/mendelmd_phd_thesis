journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Axonemal Dyneins,Base Sequence,Chromosome Mapping,Chromosomes,Complementary,DNA,Dyneins,Dyneins: genetics,Human,Humans,Kartagener Syndrome,Kartagener Syndrome: enzymology,Kartagener Syndrome: genetics,Male,Missense,Molecular Sequence Data,Mutation,Pair 7,Situs Inversus,Situs Inversus: enzymology,Situs Inversus: genetics},
