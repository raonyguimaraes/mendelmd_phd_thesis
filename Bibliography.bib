isbn = {1549-5469 (Electronic)$\backslash$n1088-9051 (Linking)},
issn = {1549-5469},
journal = {Genome research},
keywords = {Algorithms,Autistic Disorder,Autistic Disorder: genetics,Computational Biology,Computational Biology: methods,DNA Copy Number Variations,Exome,Exons,Genetic Loci,Genome,Genotyping Techniques,Genotyping Techniques: methods,HapMap Project,Human,Humans,Reproducibility of Results,Sensitivity and Specificity,User-Computer Interface},
month = {aug},
