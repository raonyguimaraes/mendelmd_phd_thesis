journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Databases,Genetic,Genetic Predisposition to Disease,Genetic Predisposition to Disease: genetics,Genome,Genome-Wide Association Study,Human,Human: genetics,Humans,Internet,Mutation},
month = {jun},
