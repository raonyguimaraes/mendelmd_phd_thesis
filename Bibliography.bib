journal = {Functional {\&} integrative genomics},
keywords = {Animals,Cattle,Cattle: genetics,Comparative Genomic Hybridization,Comparative Genomic Hybridization: methods,DNA,DNA Copy Number Variations,DNA: methods,Evolution,Genome,Genome: genetics,Genomic,Molecular,Segmental Duplications,Sequence Analysis,array comparative genomic,breed,cattle genome,copy number variation,hybridization,next-generation sequencing,segmental duplication},
