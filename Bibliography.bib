journal = {BMJ open},
month = {jan},
number = {6},
pmid = {23148340},
title = {{Who is more likely to use doctor-rating websites, and why? A cross-sectional study in London.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3532991{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {2},
year = {2012}
