pmid = {18251993},
title = {{A comparison of common programming languages used in bioinformatics.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2267699{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {9},
