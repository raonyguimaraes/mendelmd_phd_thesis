title = {{Finding disease variants in Mendelian disorders by using sequence data: methods and applications.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3234377{\&}tool=pmcentrez{\&}rendertype=abstract http://dx.doi.org/10.1016/j.ajhg.2011.11.003},
