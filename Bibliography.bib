isbn = {1097-4172 (Electronic)$\backslash$n0092-8674 (Linking)},
issn = {00928674},
journal = {Cell},
number = {6},
pages = {1293--1307},
pmid = {22424236},
publisher = {Elsevier Inc.},
title = {{Personal omics profiling reveals dynamic molecular and medical phenotypes}},
