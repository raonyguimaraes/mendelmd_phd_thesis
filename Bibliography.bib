isbn = {1591840562},
pages = {226},
title = {{The Art of the Start: The Time-tested, Battle-hardened Guide for Anyone Starting Anything}},
url = {http://books.google.co.uk/books/about/The{\_}Art{\_}of{\_}the{\_}Start.html?id=-gXlwJnnNoEC{\&}pgis=1},
year = {2004}
}
