month = {jan},
number = {1},
pages = {2},
pmid = {19348689},
title = {{Systems medicine: the future of medical genomics and healthcare.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2651587{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {1},
year = {2009}
}
