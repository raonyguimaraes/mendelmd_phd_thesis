issn = {0099-2240},
journal = {Applied and environmental microbiology},
keywords = {Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial Proteins: metabolism,Cell Wall,Cell Wall: genetics,Gene Expression Profiling,Gene Expression Regulation,Genes,Genetic,Genome,Listeria monocytogenes,Listeria monocytogenes: classification,Listeria monocytogenes: genetics,Listeria monocytogenes: metabolism,Listeria monocytogenes: pathogenicity,Metabolic Networks and Pathways,Sigma Factor,Sigma Factor: genetics,Sigma Factor: metabolism,Transcription,Virulence,Virulence: genetics},
month = {oct},
