isbn = {1662-5099 (Electronic)$\backslash$r1662-5099 (Linking)},
issn = {1662-5099},
journal = {Frontiers in molecular neuroscience},
keywords = {are pentameric chloride,channels,cl,epilepsy,expression,gaba,glycine receptor genes and,glycine receptors,glyrs,hippocampus,in brain,of neuron fi ring,rna editing,rna splicing,subunits,they contribute to inhibition},
number = {November},
