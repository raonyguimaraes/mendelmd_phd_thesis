@article{Pollettini2011,
abstract = {Pesquisas recentes em medicina gen{\^{o}}mica sugerem que fatores de risco que incidem desde a concep{\c{c}}{\~{a}}o de uma crian{\c{c}}a at{\'{e}} o final de sua adolesc{\^{e}}ncia podem influenciar no desenvolvimento de doen{\c{c}}as cr{\^{o}}nicas da idade adulta. Artigos cient{\'{i}}ficos com descobertas e estudos inovadores sobre o tema indicam que a epigen{\'{e}}tica deve ser explorada para prevenir doen{\c{c}}as de alta preval{\^{e}}ncia como doen{\c{c}}as cardiovasculares, diabetes e obesidade. A grande quantidade de artigos disponibilizados diariamente dificulta a atualiza{\c{c}}{\~{a}}o de profissionais, uma vez que buscas por informa{\c{c}}{\~{a}}o exata se tornam complexas e dispendiosas em rela{\c{c}}{\~{a}}o ao tempo gasto na procura e an{\'{a}}lise dos resultados. Algumas tecnologias e t{\'{e}}cnicas computacionais podem apoiar a manipula{\c{c}}{\~{a}}o dos grandes reposit{\'{o}}rios de informa{\c{c}}{\~{o}}es biom{\'{e}}dicas, assim como a gera{\c{c}}{\~{a}}o de conhecimento. O presente trabalho pesquisa a descoberta autom{\'{a}}tica de artigos cient{\'{i}}ficos que relacionem doen{\c{c}}as cr{\^{o}}nicas e fatores de risco para as mesmas em registros cl{\'{i}}nicos de pacientes. Este trabalho tamb{\'{e}}m apresenta o desenvolvimento de um arcabou{\c{c}}o de software para sistemas de vigil{\^{a}}ncia que alertem profissionais de sa{\'{u}}de sobre problemas no desenvolvimento humano. A efetiva transforma{\c{c}}{\~{a}}o dos resultados de pesquisas biom{\'{e}}dicas em conhecimento poss{\'{i}}vel de ser utilizado para beneficiar a sa{\'{u}}de p{\'{u}}blica tem sido considerada um dom{\'{i}}nio importante da inform{\'{a}}tica. Este dom{\'{i}}nio {\'{e}} denominado Bioinform{\'{a}}tica Translacional (BUTTE,2008). Considerando-se que doen{\c{c}}as cr{\^{o}}nicas s{\~{a}}o, mundialmente, um problema s{\'{e}}rio de sa{\'{u}}de e lideram as causas de mortalidade com 60{\%} de todas as mortes, o presente trabalho poder{\'{a}} possibilitar o uso direto dos resultados dessas pesquisas na sa{\'{u}}de p{\'{u}}blica e pode ser considerado um trabalho de Bioinform{\'{a}}tica Translacional.},
author = {Pollettini, Juliana Tarossi},
