journal = {Briefings in bioinformatics},
month = {sep},
number = {5},
pages = {528--37},
pmid = {23803301},
title = {{Best practices in bioinformatics training for life scientists.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3771230{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {14},
year = {2013}
}
