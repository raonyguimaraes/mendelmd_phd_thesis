isbn = {1875-2284 (Electronic)},
issn = {18752292},
journal = {Cancer Microenvironment},
keywords = {Cancer stem cell,Prostate,Prostate cancer,Reversion-inducing cysteine-rich protein with Kaza,Tumor-associated stroma},
