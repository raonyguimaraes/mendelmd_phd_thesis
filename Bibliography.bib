@article{Cohen2002,
abstract = {Pituitary gland commitment from oral ectoderm occurs in response to inductive signals from the neuroepithelium of the ventral diencephalon. Invagination of the oral ectoderm leads to the creation of Rathke's pouch. Intensified cell proliferation within Rathke's pouch results in formation of the anterior pituitary lobe. Subsequently, highly differentiated cell types arise sequentially due to overlapping, but distinct, spatial and temporal patterns of signaling molecules and transcription factors. Mutations in some of the pituitary-specific transcription factors have been identified in patients with hypopituitarism, confirming the role of these factors in pituitary development.},
