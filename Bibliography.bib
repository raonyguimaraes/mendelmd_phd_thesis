}
@article{Goddard2013a,
abstract = {PURPOSE: The aim of this study was to develop, operationalize, and pilot test a transparent, reproducible, and evidence-informed method to determine when to report incidental findings from next-generation sequencing technologies.$\backslash$n$\backslash$nMETHODS: Using evidence-based principles, we proposed a three-stage process. Stage I "rules out" incidental findings below a minimal threshold of evidence and is evaluated using inter-rater agreement and comparison with an expert-based approach. Stage II documents criteria for clinical actionability using a standardized approach to allow experts to consistently consider and recommend whether results should be routinely reported (stage III). We used expert opinion to determine the face validity of stages II and III using three case studies. We evaluated the time and effort for stages I and II.$\backslash$n$\backslash$nRESULTS: For stage I, we assessed 99 conditions and found high inter-rater agreement (89{\%}), and strong agreement with a separate expert-based method. Case studies for familial adenomatous polyposis, hereditary hemochromatosis, and $\alpha$1-antitrypsin deficiency were all recommended for routine reporting as incidental findings. The method requires {\textless}3 days per topic.$\backslash$n$\backslash$nCONCLUSION: We establish an operational definition of clinically actionable incidental findings and provide documentation and pilot testing of a feasible method that is scalable to the whole genome.},
author = {Goddard, Katrina a B and Whitlock, Evelyn P and Berg, Jonathan S and Williams, Marc S and Webber, Elizabeth M and Webster, Jennifer a and Lin, Jennifer S and Schrader, Kasmintan a and Campos-Outcalt, Doug and Offit, Kenneth and Feigelson, Heather Spencer and Hollombe, Celine},
doi = {10.1038/gim.2013.37},
file = {:home/raony/Articles/Description and pilot results from a novel method for evaluating return of incidental findings from next-generation sequencing technologies.pdf:pdf},
isbn = {1530-0366 (Electronic)$\backslash$r1098-3600 (Linking)},
