@article{Sirmaci2010,
abstract = {Distinctive facial features consisting of hypertelorism, telecanthus, blepharophimosis, blepharoptosis, epicanthus inversus, periumbilical defects, and skeletal anomalies are seen in autosomal-recessive Carnevale, Malpuech, Michels, and oculo-skeletal-abdominal (OSA) syndromes. The gene or genes responsible for these syndromes were heretofore unknown. We report on three individuals from two consanguineous Turkish families with findings characteristic of these syndromes, including facial dysmorphism, periumbilical depression, mixed hearing loss, radioulnar synostosis, and coccygeal appendage. Homozygosity mapping yielded an autozygous region on chromosome 3q27 in both families. In one family, whole exome sequencing revealed a missense mutation, MASP1 c.2059G{\textgreater}A (p.G687R), that cosegregated with the phenotype. In the second family, Sanger sequencing of MASP1 revealed a nonsense mutation, MASP1 c.870G{\textgreater}A (p.W290X), that also cosegregated with the phenotype. Neither mutation was found in 192 Turkish controls or 1200 controls of various other ancestries. MASP1 encodes mannan-binding lectin serine protease 1. The two mutations occur in a MASP1 isoform that has been reported to process IGFBP-5, thereby playing a critical role in insulin growth factor availability during craniofacial and muscle development. These results implicate mutations of MASP1 as the cause of a human malformation syndrome and demonstrate the involvement of MASP1 in facial, umbilical, and ear development during the embryonic period.},
author = {Sirmaci, Asli and Walsh, Tom and Akay, Hatice and Spiliopoulos, Michail and Sakalar, Yıldırım Bayezit and Hasanefendioğlu-Bayrak, Aylin and Duman, Duygu and Farooq, Amjad and King, Mary-Claire and Tekin, Mustafa},
doi = {10.1016/j.ajhg.2010.09.018},
file = {:home/raony/Articles/exome studies/1-s2.0-S0002929710005185-main.pdf:pdf},
issn = {1537-6605},
journal = {American journal of human genetics},
keywords = {Abdominal Muscles,Abdominal Muscles: abnormalities,Abnormalities,Adolescent,Amino Acid Sequence,Blepharoptosis,Blepharoptosis: genetics,Child,Congenital,Congenital: genetics,Craniofacial Abnormalities,Craniofacial Abnormalities: genetics,Craniosynostoses,Craniosynostoses: genetics,Cryptorchidism,Cryptorchidism: genetics,Developmental Disabilities,Developmental Disabilities: genetics,Eye Abnormalities,Eye Abnormalities: genetics,Face,Face: abnormalities,Female,Genotype,Hearing Disorders,Hearing Disorders: genetics,Heart Defects,Hip Dislocation,Humans,Mannose-Binding Protein-Associated Serine Protease,Molecular Sequence Data,Multiple,Multiple: genetics,Mutation,Sacrococcygeal Region,Sacrococcygeal Region: abnormalities,Strabismus,Strabismus: genetics,Umbilicus,Umbilicus: abnormalities},
