journal = {PLoS computational biology},
keywords = {Biological,Biological: metabolism,Disease Management,Humans,Neoplasms,Neoplasms: diagnosis,Neoplasms: metabolism,Neoplasms: therapy,Predictive Value of Tests,Risk Factors,Stochastic Processes,Systems Biology,Tumor Markers},
month = {feb},
number = {2},
pages = {e1001087},
pmid = {21390279},
title = {{Application of biomarkers in cancer risk management: evaluation from stochastic clonal evolutionary and dynamic system optimization points of view.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3044766{\&}tool=pmcentrez{\&}rendertype=abstract},
