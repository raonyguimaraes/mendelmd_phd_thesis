pmid = {11742071},
title = {{Multiclass cancer diagnosis using tumor gene expression signatures.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=64998{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {98},
