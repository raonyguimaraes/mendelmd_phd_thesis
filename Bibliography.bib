@article{Barak2011,
abstract = {The biological basis for regional and inter-species differences in cerebral cortical morphology is poorly understood. We focused on consanguineous Turkish families with a single affected member with complex bilateral occipital cortical gyration abnormalities. By using whole-exome sequencing, we initially identified a homozygous 2-bp deletion in LAMC3, the laminin $\gamma$3 gene, leading to an immediate premature termination codon. In two other affected individuals with nearly identical phenotypes, we identified a homozygous nonsense mutation and a compound heterozygous mutation. In human but not mouse fetal brain, LAMC3 is enriched in postmitotic cortical plate neurons, localizing primarily to the somatodendritic compartment. LAMC3 expression peaks between late gestation and late infancy, paralleling the expression of molecules that are important in dendritogenesis and synapse formation. The discovery of the molecular basis of this unusual occipital malformation furthers our understanding of the complex biology underlying the formation of cortical gyrations.},
author = {Barak, Tanyeri and Kwan, Kenneth Y and Louvi, Angeliki and Demirbilek, Veysi and Saygı, Serap and T{\"{u}}ys{\"{u}}z, Beyhan and Choi, Murim and Boyacı, H{\"{u}}seyin and Doerschner, Katja and Zhu, Ying and Kaymak{\c{c}}alan, Hande and Yılmaz, Saliha and Bakırcıoğlu, Mehmet and {\c{C}}ağlayan, Ahmet Okay and {\"{O}}zt{\"{u}}rk, Ali Kemal and Yasuno, Katsuhito and Brunken, William J and Atalar, Ergin and Yal{\c{c}}ınkaya, Cengiz and Din{\c{c}}er, Alp and Bronen, Richard A and Mane, Shrikant and {\"{O}}z{\c{c}}elik, Tayfun and Lifton, Richard P and {\v{S}}estan, Nenad and Bilg{\"{u}}var, Kaya and G{\"{u}}nel, Murat and Cağlayan, Ahmet Okay and Ozt{\"{u}}rk, Ali Kemal and Oz{\c{c}}elik, Tayfun and Sestan, Nenad},
