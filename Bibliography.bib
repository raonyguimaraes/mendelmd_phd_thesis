journal = {American Journal of Medical Genetics, Part A},
keywords = {Diagnostics,Exome sequencing and recessive disorders,Liver disease,Sensenbrenner syndrome,Short rib polydactyly},
number = {11},
pages = {2917--2924},
pmid = {22987818},
title = {{WDR35 mutation in siblings with Sensenbrenner syndrome: A ciliopathy with variable phenotype}},
volume = {158 A},
year = {2012}
}
