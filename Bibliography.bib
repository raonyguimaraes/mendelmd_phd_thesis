journal = {Molecular and biochemical parasitology},
keywords = {Adolescent,Adult,Animals,Chagas Cardiomyopathy,Chagas Cardiomyopathy: parasitology,Chagas Disease,Chagas Disease: parasitology,DNA, Kinetoplast,DNA, Kinetoplast: genetics,Female,Gene Deletion,Gene Expression Profiling,Gene Expression Regulation,Humans,Male,Membrane Proteins,Membrane Proteins: genetics,Middle Aged,Molecular Sequence Data,NADH Dehydrogenase,NADH Dehydrogenase: genetics,Oligonucleotide Array Sequence Analysis,Protozoan Proteins,Protozoan Proteins: genetics,Trypanosoma cruzi,Trypanosoma cruzi: genetics},
month = {dec},
