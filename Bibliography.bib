issn = {1537-6605},
journal = {American journal of human genetics},
keywords = {Adolescent,Adult,Agenesis of Corpus Callosum,Agenesis of Corpus Callosum: genetics,Agenesis of Corpus Callosum: pathology,Arachnoid Cysts,Arachnoid Cysts: genetics,Arachnoid Cysts: pathology,Brain,Brain Diseases,Brain Diseases: genetics,Brain: abnormalities,Brain: pathology,Child,DNA,Family Health,Female,Gene Deletion,Hearing Loss,Homozygote,Humans,Infant,Intracellular Signaling Peptides and Proteins,Intracellular Signaling Peptides and Proteins: gen,Male,Mutation,Preschool,Sensorineural,Sensorineural: genetics,Sensorineural: pathology,Sequence Analysis},
month = {jun},
