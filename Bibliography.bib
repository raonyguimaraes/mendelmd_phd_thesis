journal = {Acta Chirurgica Iugoslavica},
keywords = {Humans,Index Medicus,Minimally Invasive Surgical Procedures,Pilonidal Sinus/su [Surgery]},
number = {2},
pages = {81--85},
pmid = {23373363},
title = {{Minor vs complete excision of pilonidal sinus--early postoperative period.}},
url = {http://sfxhosted.exlibrisgroup.com/calgary?sid=OVID:Ovid+MEDLINE(R){\&}id=pmid:23373363{\&}id=doi:{\&}issn=0354-950X{\&}isbn={\&}volume=59{\&}issue=2{\&}spage=81{\&}pages=81-5{\&}date=2012{\&}title=Acta+Chirurgica+Iugoslavica{\&}atitle=Minor+vs+complete+excision+of+pilonidal+sinus--e},
