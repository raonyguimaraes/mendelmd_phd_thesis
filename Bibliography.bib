issn = {1932-6203},
journal = {PloS one},
keywords = {Brazil,Brazil: ethnology,Cohort Studies,Continental Population Groups,Continental Population Groups: ethnology,Continental Population Groups: genetics,Cytochrome P-450 CYP3A,Cytochrome P-450 CYP3A: genetics,Female,Gene Frequency,Genome, Human,Genotype,Geography,Humans,Male,Metagenomics,Mixed Function Oxygenases,Mixed Function Oxygenases: genetics,Phylogeography,Skin Pigmentation,Skin Pigmentation: genetics,Skin Pigmentation: physiology,Vitamin K Epoxide Reductases},
month = {jan},
