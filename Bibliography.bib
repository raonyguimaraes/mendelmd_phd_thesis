journal = {Neuropsychopharmacology : official publication of the American College of Neuropsychopharmacology},
keywords = {Adult,Attention,Attention: drug effects,Cognition,Cognition: drug effects,Double-Blind Method,Female,Hallucinogens,Hallucinogens: pharmacology,Heart Rate,Heart Rate: drug effects,Humans,Male,Marijuana Smoking,Marijuana Smoking: psychology,Memory,Memory: drug effects,Mental Recall,Mental Recall: drug effects,Psychomotor Performance,Psychomotor Performance: drug effects,Reaction Time,Reaction Time: drug effects,Space Perception,Space Perception: drug effects,Tetrahydrocannabinol,Tetrahydrocannabinol: pharmacology},
month = {nov},
