number = {7435},
pages = {77--80},
pmid = {23354052},
publisher = {Nature Publishing Group},
title = {{Towards practical, high-capacity, low-maintenance information storage in synthesized DNA.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3672958{\&}tool=pmcentrez{\&}rendertype=abstract},
