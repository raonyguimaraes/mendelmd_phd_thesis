issn = {1524-4539},
journal = {Circulation},
keywords = {Calmodulin,Calmodulin: genetics,Female,Heart Arrest,Heart Arrest: genetics,Humans,Long QT Syndrome,Long QT Syndrome: genetics,Male},
month = {mar},
number = {9},
pages = {968--70},
pmid = {23388216},
title = {{Using whole exome sequencing to walk from clinical practice to research and back again.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23388216},
volume = {127},
year = {2013}
}
